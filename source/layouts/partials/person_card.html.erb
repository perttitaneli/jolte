<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
    Taulu <%= current_page.data.try(:taulu) %>
    <h1><%= current_page.data.etunimet %> <%= current_page.data.sukunimi %></h1>
    <% if current_page.data.try(:syntyi) %>
        <%= current_page.data.try(:syntyi) %> <%= current_page.data.try(:syntypaikka) %>
        - <%= current_page.data.try(:kuoli) %>
    <% end %>
  </div>
  <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
    <h4>Vanhemmat</h4>
    <ul>
      <li>
        <% search_name = search_name(current_page) %>
        <%= taululinkki father(search_name) %>
      </li>
      <li>
        <%= taululinkki mother(search_name) %>
      </li>
    </ul>
  </div>
</div>